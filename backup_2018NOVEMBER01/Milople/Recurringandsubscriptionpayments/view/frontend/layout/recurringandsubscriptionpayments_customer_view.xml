<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <css src="Milople_Recurringandsubscriptionpayments::css/main.css" />
   </head>
    <body> 
 	<referenceBlock name="page.main.title" remove="true">
          <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Details</argument>
            </action> 
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Milople\Recurringandsubscriptionpayments\Block\Customer\View" name="recurringandsubscriptionpayments.customer.subscription.view" cacheable="false">
            </block>
            <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back" template="account/link/back.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>

